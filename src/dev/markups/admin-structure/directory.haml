#bems-admin-structure-modal-delete.bems-admin-structure-modal-delete.modal.fade{:taboverview => "-1", :role => "dialog", "aria-labellyby" => "bemsModal"}
  .modal-dialog{:role => "document"}
    .modal-content
      .modal-header
        %button.close.bems-admin-structure-modal-delete-close{"aria-hidden" => "true", "data-dismiss" => "modal", :type => "button"} ×
        %h4.modal-title.bems-about-title
          %i.material-icons delete_forever
          Do you want to delete {{delName}} ?
      .modal-footer.bems-admin-structure-modal-delete-footer.text-center
        %button.btn.btn-success{:type => "button", "data-dismiss" => "modal", "ng-click" => "deleteNode(delId)"} Confirm
        %button.btn.btn-default{:type => "button", "data-dismiss" => "modal"} Cancel
