.col-xs-12
  %treecontrol.tree-boot{"tree-model" => "data", :options => "tOps"}
    .tree-label-inner{"ng-class" => "{'bems-node-selected':detailNode.id == node.id}", :title => "{{node.name}}"}
      .tree-label-wrapper
        .row
          .col-xs-6 {{node.name}}
          .col-xs-6
            %a.pull-right.btn.btn-primary.btn-sm{"ng-click" => "newSubNode(node.id,node.name)"}
              %span.glyphicon.glyphicon-plus
            %a.pull-right.btn.btn-info.btn-sm{"ng-click" => "editNode(node.id)"}
              %span.glyphicon.glyphicon-edit
            %a.pull-right.btn.btn-danger.btn-sm{"ng-click" => "confirmDeleteNode(node.id,node.name)", "ng-hide" => "node.id === firstNode"}
              %span.glyphicon.glyphicon-remove
